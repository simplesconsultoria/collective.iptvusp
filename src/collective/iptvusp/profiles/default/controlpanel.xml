<?xml version="1.0"?>
<object name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.iptvusp"
    purge="False">
    <configlet title="IPTV USP Settings"
        action_id="iptvusp"
        appId="collective.iptvusp"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@iptvusp-settings"
        icon_expr="string:++resource++collective.iptvusp/video_icon.png"
        visible="True"
        i18n:attributes="title">
        <permission>Manage portal</permission>
    </configlet>
</object>
